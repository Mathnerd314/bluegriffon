
domexplorer.jar:
% content   domexplorer          %content/domexplorer/
% skin      domexplorer         classic/1.0 %skin/classic/domexplorer/
% overlay   chrome://bluegriffon/content/xul/bluegriffon.xul    chrome://domexplorer/content/domexplorerOverlay.xul
% style     chrome://bluegriffon/content/xul/bluegriffon.xul    chrome://domexplorer/skin/domexplorerOverlay.css
* content/domexplorer/domexplorerOverlay.xul      (content/domexplorerOverlay.xul)
  content/domexplorer/domexplorerOverlay.js       (content/domexplorerOverlay.js)
* content/domexplorer/domexplorer.xul             (content/domexplorer.xul)
* content/domexplorer/domexplorer.js              (content/domexplorer.js)
* skin/classic/domexplorer/domexplorer.css        (skin/domexplorer.css)
  skin/classic/domexplorer/domexplorer.png        (skin/domexplorer.png)
  skin/classic/domexplorer/small/domexplorer.png  (skin/small/domexplorer.png)

domexplorer-locale.jar:
% locale    domexplorer    en-US    %locale/en-US/domexplorer/
% locale    domexplorer    fr-FR    %locale/fr-FR/domexplorer/
  locale/en-US/domexplorer/domexplorerOverlay.dtd (locale/en-US/domexplorerOverlay.dtd)
  locale/en-US/domexplorer/domexplorer.dtd        (locale/en-US/domexplorer.dtd)
  locale/fr-FR/domexplorer/domexplorerOverlay.dtd (locale/fr-FR/domexplorerOverlay.dtd)
  locale/fr-FR/domexplorer/domexplorer.dtd        (locale/fr-FR/domexplorer.dtd)
  